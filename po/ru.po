"исследований (ОИЯИ), Россия."

#: ../libelemental/data.cc:168
msgid "Tennessine"
msgstr "Теннессин"

#: ../libelemental/data.cc:169
msgid "Oganesson"
msgstr "Оганесон"

#~ msgid "Ununhexium"
#~ msgstr "Унунгексий"

#~ msgid "Ununquadium"
#~ msgstr "Унунквадий"
